@model TopicIndexMorePage

@{
	// Removes the theme from the page because this result is intended to be embedded through script.
	Layout = null;
}

@foreach (var item in Model.Topics) {
	<partial name="_MergeItem" for="@item" />
}

<script>
	window.page = @Model.Page;
	window.moreTopics = @Model.More.ToString().ToLower();

	window.BindLinks();
</script>